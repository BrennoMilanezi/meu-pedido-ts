<div class="row">
    <div class="col s8">
        <div class="row">
            <% produtos.bebidas.forEach(function (bebida) { %>
            <div class="col-s6 caixa-respon" >
                <div class="row">
                    <div class="col-s12">
                        <div class="card blue-grey darken-1">
                            <div class="card-content white-text">
                              <p><%= bebida.nome_produto %><i style="margin-left: 25%;cursor: pointer;" class="small material-icons">stars</i></p>
                            </div>
                            <div class="card-action">
                                <p><%= bebida.qnt_produto %> Unds.</p>
                                <p>R$ <%= bebida.preco_produto %><i style="margin-left: 25%;cursor: pointer;" class="small material-icons">add_circle</i></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <% }); %>
        
        </div>
    </div>
    <div class="col s4 caixa-finalizar" style="background: #b9b5b5d4;">
        <div class="row">
            
            <div class="col-s12">
                <div class="card blue-grey darken-1">
                    <div class="card-content white-text">
                          <p>COXINHA</p>
                    </div>
                    <div class="card-action">
                        <p>33 Unds.</p>
                        <p>R$ 13,00<i style="margin-left: 46%;cursor: pointer;" class="small material-icons">do_not_disturb_on</i></p>
                    </div>
                </div>
            </div>
        </div>
        
        <div class="row">
            
            <div class="col-s12">
                <div class="card blue-grey darken-1">
                    <div class="card-content white-text">
                          <p>Total</p>
                    </div>
                    <div class="card-action">
                        <p>R$ 8001,00</p>
                    </div>
                    <div class="card-action">
                        <a class="waves-effect waves-light btn" href="/pagamento">Finalizar</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>