<!DOCTYPE html>
<html>
  <head>
    <title>Perfil</title>
    <link rel='stylesheet' href='/stylesheets/style.css?v=1.0' />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <%- include('template/head') %>
    <script type="text/javascript" src="https://www.superestagios.com.br/index/js/jquery.js"></script>
    <script type="text/javascript" src="https://www.superestagios.com.br/js/jquery.snow.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script type='text/javascript'>
        $(document).ready(function() {
            $('#cpf').keypress(function() {
                formatar(this, '###.###.###-##');
                return bloqueaAlfaNumericos();
    
            });
        });
    	$(document).ready(function() {
            $('#data_nascimento').keypress(function() {
                formatar(this, '##/##/####');
                return bloqueaAlfaNumericos();
    
            });
        });
    	$(document).ready(function() {
            $('#telefone').keypress(function() {
                formatar(this, '##-#####-####');
                return bloqueaAlfaNumericos();
    
            });
        });
        function formatar(src, mask) {
            var i = src.value.length;
            var saida = mask.substring(0, 1);
            var texto = mask.substring(i)
            if (texto.substring(0, 1) != saida) {
                src.value += texto.substring(0, 1);
            }
        }
        function bloqueaAlfaNumericos() {
            var tecla = window.event.keyCode;
            tecla = String.fromCharCode(tecla);
            if (((tecla >= '0') && (tecla <= '9')) || window.event.keyCode == 13) {
                return true;
    
            } else {
                return false;
    
            }
        }
    </script>
  </head>
  <body class="login">

    <h3 class="title center-align">Perfil</h3>
    <hr>
    <form>
      <div class="row">
        <div class="col-s13-m10">
           
           <div class="card home-respon">
            <div class="card-content">
            </div>
            <div class="card-tabs">
                <ul class="tabs tabs-fixed-width">
                    <li class="tab"><a class="active" href="#dados">Dados</a></li>
                    <li class="tab"><a href="#list_pedidos">Lista de Pedidos</a></li>
                    <li class="tab"><a href="#meus_cartoes">Meus Cart√µes</a></li>
                </ul>
            </div>
            <div class="card-content grey lighten-4">
                <div id="dados"><%- include('menuProfile/dados') %></div>
                <div id="list_pedidos"><%- include('menuProfile/list_pedidos') %></div>
                <div id="meus_cartoes"><%- include('menuProfile/meus_cartoes') %></div>
            </div>
        </div>
      </div>
    </div>
  </form>
    
      
  <%- include('template/scripts') %>
  </body>
</html>